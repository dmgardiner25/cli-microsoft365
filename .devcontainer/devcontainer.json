{
  "name": "CLI for Microsoft 365",
  "dockerFile": "Dockerfile",
  "extensions": [
    "eamodio.gitlens",
    "vsls-contrib.codetour",
  ],
  "settings": {
    "terminal.integrated.shell.linux": "/bin/zsh"
  },
  "remoteUser": "cli-microsoft365-dev",
  "postCreateCommand": "npm i && npm run clean && npm run build && npm link"
}